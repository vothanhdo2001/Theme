/* Customer Page */
{%- if settings.login_page_bg != blank -%}
.login-form-container {
   background: {{ settings.login_page_bg }};
}
{%- endif -%}
{%- if settings.login_title_Color != blank -%}
.login-text h2{
   color: {{ settings.login_title_Color }};
}
{%- endif -%}
{%- if settings.login_subtitle_Color != blank -%}
.login-text p{
  color: {{ settings.login_subtitle_Color }};
}
{%- endif -%}
{%- if settings.login_action_btn_Color != blank -%}
.form-action-button a,.account-optional-action a{
   color: {{ settings.login_action_btn_Color }};
}
{%- endif -%}

{%- if settings.login_action_btn_hover_Color != blank -%}
.form-action-button a:hover,.account-optional-action a:hover{
   color: {{ settings.login_action_btn_hover_Color }};
}
{%- endif -%}

{%- if settings.input_bg_color != blank -%}
.login-form-container input{
  background: {{ settings.input_bg_color }};
}
{%- endif -%}

{%- if settings.input_text_color != blank -%}
.login-form-container input{
  color: {{settings.input_text_color}};
}
.login-form-container input::-webkit-input-placeholder { /* Chrome/Opera/Safari */
  color: {{settings.input_text_color}};
}
.login-form-container input::-moz-placeholder { /* Firefox 19+ */
  color: {{settings.input_text_color}};
}
.login-form-container input:-ms-input-placeholder { /* IE 10+ */
  color: {{settings.input_text_color}};
}
.login-form-container input:-moz-placeholder { /* Firefox 18- */
  color: {{settings.input_text_color}};
}
{%- endif -%}
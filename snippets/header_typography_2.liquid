{%- capture css -%}
<style>
  {%- for block in section.blocks -%}
  {%- case block.type -%}
  {%- when 'header_style' -%}
  
  #section-{{ section.id }} .header-bottom.header_index_new {
    background: {{ block.settings.navbar_bg_color }};
  }

  #section-{{ section.id }}.header,#section-{{ section.id }} .header-sticky.is-sticky {
    background: {{ block.settings.header_bg_color }};
  } 
  #section-{{ section.id }} nav.site-nav > ul > li > a {
    color: {{ block.settings.header_menu_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li:hover > a {
    color: {{ block.settings.header_menu_hover_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li.menu-item-has-children > a::before {
    color: {{ block.settings.dropdown_arrow_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li > a::after {
    background-color: {{ block.settings.header_menu_hover_border_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li ul.sub-menu,#section-{{ section.id }} nav.site-nav > ul > li ul.single-column-menu.single-column-has-children > li .multilevel-submenu {
    background-color: {{ block.settings.submenu_bg_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li ul.mega-menu > li a.mega-column-title {
    color: {{ block.settings.mega_item_menu_title_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li ul.mega-menu > li .mega-sub-menu li a {
    color: {{ block.settings.mega_menu_item_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li ul.mega-menu > li .mega-sub-menu li a:hover {
    color: {{ block.settings.mega_menu_hover_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li ul.mega-menu-column-5 > li .mega-sub-menu li a:hover::after {
    background-color: {{ block.settings.mega_menu_hover_border_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li ul.mega-menu > li {
    border-color: {{ block.settings.mega_item_border_color }};
  }
  
  nav.site-nav > ul > li ul.mega-menu > li > .mega-sub-menu li.single-column-has-children::before,nav.site-nav > ul > li ul.single-column-menu li.single-column-has-children::after {
    color: {{ block.settings.mega_menu_item_color }};
  }
  nav.site-nav > ul > li ul.mega-menu > li > .mega-sub-menu li.single-column-has-children .mega-sub-level2,nav.site-nav > ul > li ul.single-column-menu li.single-column-has-children .multilevel-submenu {
    background:  {{ block.settings.submenu_bg_color }};
  }


  #section-{{ section.id }} nav.site-nav > ul > li ul.single-column-menu li a {
    color: {{ block.settings.mega_menu_item_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li ul.single-column-menu li a:hover {
    color: {{ block.settings.mega_menu_hover_color }}
  }
  #section-{{ section.id }} nav.site-nav > ul > li ul.single-column-menu li a:hover::after {
    background-color: {{ block.settings.mega_menu_hover_border_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li ul.single-column-menu.single-column-has-children > li.menu-item-has-children::after {
    color: {{ block.settings.multi_dropdown_arrow_color }}
  }


  #section-{{ section.id }} nav.site-nav > ul > li a span,#section-{{ section.id }} .site-mobile-nav .dl-menu li a span {
    background: {{ block.settings.menu_badge_bg_color }};
    color: {{ block.settings.menu_badge_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li a span::before,#section-{{ section.id }} .site-mobile-nav .dl-menu li a span::before {
    background: {{ block.settings.menu_badge_bg_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li a span.sale,#section-{{ section.id }} .site-mobile-nav .dl-menu li a span.sale {
    background: {{ block.settings.menu_sale_badge_bg_color }};
    color: {{ block.settings.menu_sale_badge_color }};
  }
  #section-{{ section.id }} nav.site-nav > ul > li a span.sale:before,#section-{{ section.id }} .site-mobile-nav .dl-menu li a span.sale::before { 
    background: {{ block.settings.menu_sale_badge_bg_color }};
  }

  #section-{{ section.id }} .header-right-icons .single-icon a i {
    color: {{ block.settings.menu_account_icon_color }};
  }
  #section-{{ section.id }} .header-right-icons .single-icon a span.count {
    background-color: {{ block.settings.mini_cart_counter_bg }};
    color: {{ block.settings.mini_cart_counter_color }};
  }

  .site-mobile-nav .hamburger .hamburger-box .hamburger-inner,
  .site-mobile-nav .hamburger .hamburger-box .hamburger-inner::before, 
  .site-mobile-nav .hamburger .hamburger-box .hamburger-inner::after{
    background-color: {{ block.settings.mobile_menu_bar_color }};
  }
  #section-{{ section.id }} .site-mobile-nav .dl-menu {
    background-color: {{ block.settings.mobile_menu_bg_color }};
  }
  #section-{{ section.id }} .site-mobile-nav .dl-menu li {
    border-color: {{ block.settings.menu_item_border_color }};
  }
  #section-{{ section.id }} .site-mobile-nav .dl-menu li > a:not(:only-child)::after {
    color: {{ block.settings.mobile_menu_arrow_color }};
  }
  #section-{{ section.id }} .site-mobile-nav .dl-menu li > a {
    color: {{ block.settings.mobile_menu_item_color }};
  }
  #section-{{ section.id }}  .site-mobile-nav .dl-menu li:hover > a {
    color: {{ block.settings.mobile_menu_hover_color }};
    background-color: {{ block.settings.mobile_menu_hover_bg_color }};
  }
  #section-{{ section.id }}  .site-mobile-nav .dl-menu li:hover > a::after {
    color: {{ block.settings.hover_dropdown_arrow_color }};
  }
  {%- when 'topbar_style' -%}
  
  #section-{{ section.id }} .order-online-text {
    color: {{ block.settings.phone_lable_color }};
  }
  #section-{{ section.id }} .order-online-text a {
    color: {{ block.settings.phone_color }};
    border-color: {{ block.settings.phone_color }};
  }
  #section-{{ section.id }} .change-dropdown .currency-trigger {
    color: {{ block.settings.dropdown_label_color }};
  }
  #section-{{ section.id }} .change-dropdown .currency-trigger:hover {
    color: {{ block.settings.dropdown_label_hover_color }};
  }
  #section-{{ section.id }} .change-dropdown ul {
    background-color: {{ block.settings.dropdown_bg_color }};
  }
  #section-{{ section.id }}.change-dropdown ul li a:not([href]):not([tabindex]),#section-{{ section.id }} .change-dropdown ul li a {
    color: {{ block.settings.dropdown_text_color }};
  }
  #section-{{ section.id }} .change-dropdown ul li a:not([href]):not([tabindex]):hover,#section-{{ section.id }} .change-dropdown ul li a:hover {
    color: {{ block.settings.dropdown_text_hover_color }};
  }
  #section-{{ section.id }} .change-dropdown > a {
    color:  {{ block.settings.dropdown_label_color }};
  }
  #section-{{ section.id }} .change-dropdown > a:hover {
    color: {{ block.settings.dropdown_label_hover_color }};
  }
  #section-{{ section.id }} .header-top-container .header-top-right .top-social-icons ul li a {
    color: {{ block.settings.social_icon_color }};
  }
  #section-{{ section.id }} .header-top .header-separator {
    color: {{ block.settings.separator_color }};
  }
 
  /* Header topbar new color support */
  #section-{{ section.id }} .topbar_account_info > a {
    color: {{ block.settings.topbar_account_info_text_color }};
  }
  #section-{{ section.id }} .topbar_account_info > a:hover {
    color: {{ block.settings.topbar_account_info_text_hover_color }};
  }
  #section-{{ section.id }} .topbar_account_info {
    color: {{ block.settings.or_text_color }};  
  }
  #section-{{ section.id }} .header-top-container .header-top-right .top-social-icons ul li a:hover {
    color: {{ block.settings.social_icon_hover_color }};
  }
  {%- when 'headerMiddle' -%}
  {%- assign bisset = block.settings -%}
  #section-{{ section.id }} .header_middle {
    background: {{ bisset.header_middle_bg }};
  }
  #section-{{ section.id }}  .new_header_search_bar input[type="search"] {
    background: {{ bisset.search_bg_color }};
    border-color: {{ bisset.search_border_color }};
    color: {{ bisset.search_text_color }};
  }
  #section-{{ section.id }} .new_header_search_bar button {
    color: {{ bisset.search_submit_icon_color }};
  }
  #section-{{ section.id }} .header-right-icons .mini_cart > a,#section-{{ section.id }} .header-right-icons .wishlist_icon > a {
    color: {{ bisset.single_account_info_color }};
  }
   #section-{{ section.id }} .header-right-icons .mini_cart > a:hover, #section-{{ section.id }} .header-right-icons .wishlist_icon > a:hover {
    color: {{ bisset.single_account_info_hover_color }};
  }
  #section-{{ section.id }} .header_middle_inner .mini_cart > a span.count {
    background-color: {{ bisset.cart_count_bg }};
    color: {{ bisset.cart_count_Text_color }};
  } 
  {%- when 'megamenu' -%}
  #block-{{ block.id }} .mega_banner_content .title1 {
    color: {{ block.settings.banner_title1 }};
  }
  #block-{{ block.id }} .mega_banner_content .title2 {
    color: {{ block.settings.banner_title2 }};
  }
  #block-{{ block.id }} .mega_banner_content .content {
    color: {{ block.settings.description_color }};
  }
  #block-{{ block.id }} .mega_banner_content .banner__btn {
    color: {{ block.settings.banner_btn_color }};
  }
  #block-{{ block.id }} .product_menu_title {
    color: {{ block.settings.product_menu_titie_color }};
  }
  #block-{{ block.id }} .menu_sin_product .product-title a{
    color: {{ block.settings.product_title_color }};
  }
  #block-{{ block.id }} .menu_sin_product .product__price span{
    color: {{ block.settings.product_price_color }};
  }
  #block-{{ block.id }} .menu_sin_product .product__price .main-price.discounted span{
    color: {{ block.settings.product_old_price_color }};
  }
  #block-{{ block.id }} .menu_product_contain .owl-nav > div {
    color: {{ block.settings.arrow_btn_color }};
    background: {{ block.settings.arrow_btn_bg_color }};
  }
  {%- when 'offcanvas_color' -%}
   #section-{{ section.id }} .offcanvas-menu-close {
    background: {{ block.settings.close_icon_bg_color }};
    color: {{ block.settings.close_icon_text_color }};
  }
  #section-{{ section.id }} .mobile-navigation-icon i,#section-{{ section.id }} .mobile-navigation-icon i:before,#section-{{ section.id }} .mobile-navigation-icon i:after  {
    background-color: {{ block.settings.mobile_hamburger_icon }};
  }
  
  #section-{{ section.id }} .offcanvas-wrapper {
    background-color: {{ block.settings.off_menu_bg_color }};
  }
  #section-{{ section.id }} .offcanvas-navigation > ul > li > a,#section-{{ section.id }} .offcanvas-navigation ul.sub-menu > li > a,
  #section-{{ section.id }} .offcanvas-navigation ul li.menu-item-has-children .menu-expand {
    color: {{ block.settings.off_can_menu_color }};
  }
  #section-{{ section.id }} .offcanvas-navigation > ul > li > a:hover,#section-{{ section.id }} .offcanvas-navigation ul.sub-menu > li > a:hover {
    color: {{ block.settings.off_can_menu_hover_color }};
  }
   #section-{{ section.id }} .offcanvas-navigation > ul li a span {
    background: {{ block.settings.menu_badge_bg_color }};
    color: {{ block.settings.menu_badge_color }};
  }
  #section-{{ section.id }} .offcanvas-navigation>ul li a span::before {
    background: {{ block.settings.menu_badge_bg_color }};
  }
  
  #section-{{ section.id }} .offcanvas-navigation>ul li a span.sale{
    background: {{ block.settings.menu_sale_badge_bg_color }};
    color: {{ block.settings.menu_sale_badge_color }};
  }
  #section-{{ section.id }} .offcanvas-navigation>ul li a span.sale::before { 
    background: {{ block.settings.menu_sale_badge_bg_color }};
  }
  #section-{{ section.id }} .offcanvas-mobile-search-area input {
    background: {{ block.settings.input_bg_color }};
    color: {{ block.settings.input_text_color }};
  }
  #section-{{ section.id }} .offcanvas-mobile-search-area button {
    color: {{ block.settings.submit_btn_color }};
  }

  #section-{{ section.id }} .offcanvas-mobile-menu .social-icons--footer ul li a {
    color: {{ block.settings.socila_icon_color }};
  }
   {%- when 'mini_cart' -%}
  /*Checkout Button*/
  .offcanvas-cart-content-container .cart-product-wrapper .cart-buttons a.checkout_btn {
    background-color: {{ block.settings.checkout_Btn_bg_color }};
    border-color:  {{ block.settings.checkout_Btn_bg_color }};
    color:  {{ block.settings.checkout_Btn_text_color }};
  }
  .offcanvas-cart-content-container .cart-product-wrapper .cart-buttons a.checkout_btn:hover {
    background-color: {{ block.settings.checkout_Btn_hover_bg_color }};
    border-color:  {{ block.settings.checkout_Btn_hover_bg_color }};
    color:  {{ block.settings.checkout_Btn_hover_text_color }};
  }
	/* Cart Button */
  
  .offcanvas-cart-content-container .cart-product-wrapper .cart-buttons a{
  	background-color: {{ block.settings.miniCart_Btn_bg_color }};
    border-color:  {{ block.settings.miniCart_Btn_bg_color }};
    color:  {{ block.settings.miniCart_Btn_text_color }};
  }
  .offcanvas-cart-content-container .cart-product-wrapper .cart-buttons a:hover {
    background-color: {{ block.settings.miniCart_Btn_hover_bg_color }};
    border-color:  {{ block.settings.miniCart_Btn_hover_bg_color }};
    color:  {{ block.settings.miniCart_Btn_hover_text_color }};
  }
  
  {%- endcase -%}
  {%- endfor -%}
  .theme-logo img{
    max-width: {{ section.settings.logo_width }}px;
  }
</style>
{%- endcapture -%}

{{ css | strip_newlines
       | replace: '  ', '' 
       | replace: ': ', ':'
       | replace: ' {', '{'
}}